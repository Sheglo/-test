length1 = int(input("введите длинну первого параллелепипеда: "))
width1 = int(input("введите ширину первого параллелепипеда: "))
height1 = int(input("введите высоту первого параллелепипеда: "))

length2 = int(input("введите длинну второго параллелепипеда: "))
width2 = int(input("введите ширину второго параллелепипеда: "))
height2 = int(input("введите высоту второго параллелепипеда: "))

V1 = length1 * width1 * height1
V2 = length2 * width2 * height2
if (V1>V2):
    print("объём первого параллелепипеда больше объёма второго параллелепипеда")
else:
    print("объём второго параллелепипеда больше объёма первого параллелепипеда")
print("объём первого параллелепипеда:",V1,"объём второго параллелепипеда:",V2 )
